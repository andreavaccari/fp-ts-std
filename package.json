{
  "name": "fp-ts-std",
  "description": "The missing pseudo-standard library for fp-ts.",
  "version": "0.14.2",
  "license": "MIT",
  "author": "Sam A. Horvath-Hunt <hello@samhh.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/samhh/fp-ts-std"
  },
  "homepage": "https://github.com/samhh/fp-ts-std",
  "bugs": "https://github.com/samhh/fp-ts-std/issues",
  "keywords": [
    "functional-programming",
    "typescript",
    "fp-ts"
  ],
  "engines": {
    "node": ">=14"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/ --format esm,cjs --clean --sourcemap --no-splitting --onSuccess 'npm run build:types'",
    "prebuild:types": "del 'dist/*.d.ts*'",
    "build:types": "tsc -p ./tsconfig.build-types.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src/ ./test/ --ext ts",
    "fmt": "prettier .",
    "test": "jest",
    "docs": "docs-ts",
    "tshm": "tshm-docs-ts ./docs/modules/*",
    "prepublish": "yarn run build"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^27.0.2",
    "@types/jsdom": "^16.2.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "del-cli": "^4.0.1",
    "docs-ts": "^0.6.7",
    "eslint-plugin-functional": "^4.0.0",
    "eslint": "^8.0.1",
    "fast-check": "^2.5.0",
    "fp-ts-contrib": "^0.1.26",
    "fp-ts-laws": "^0.3.0",
    "fp-ts": "^2.11.0",
    "jest": "^28.1.0",
    "jsdom": "^19.0.0",
    "monocle-ts": "^2.3.0",
    "newtype-ts": "^0.3.0",
    "prettier": "^2.1.2",
    "ts-jest": "^28.0.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "fp-ts": "^2.11.0",
    "monocle-ts": "^2.3.0",
    "newtype-ts": "^0.3.0"
  }
}
